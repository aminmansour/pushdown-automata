<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox fx:id="vbOutput" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <FlowPane alignment="BOTTOM_LEFT" prefHeight="30.0" prefWidth="400.0" BorderPane.alignment="CENTER">
            <children>
                <Label id="lOutput" text="Output :"/>
            </children>
            <padding>
                <Insets left="20.0" top="20.0"/>
            </padding>
        </FlowPane>
        <StackPane fx:id="spOutput" alignment="CENTER" minHeight="200.0" prefWidth="100.0"
                   BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      spacing="10">
                    <children>
                        <Label id="lConfigurationSequence" fx:id="lOutcome" text="kkkkkkkkkkkkkkkkkkkkkkkkk"
                               textAlignment="JUSTIFY" wrapText="true"/>
                    </children>
                    <StackPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </StackPane.margin>
                </HBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
            <VBox.margin>
                <Insets bottom="15.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </StackPane>
        <FlowPane alignment="CENTER" hgap="10" prefHeight="30.0" prefWidth="400.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="bAnotherInput" mnemonicParsing="false" styleClass="green-button-theme"
                        text="Another Input"/>
                <Button fx:id="bAnotherSolution" mnemonicParsing="false" styleClass="green-button-theme"
                        text="Look For Another Solution"/>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0"/>
            </BorderPane.margin>
            <padding>
                <Insets bottom="20.0"/>
            </padding>
        </FlowPane>


    </children>
</VBox>
