<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PDARunnerController" fx:id="spPDARunnerPage">
    <BorderPane maxHeight="Infinity" maxWidth="Infinity" xmlns="http://javafx.com/javafx/8.0.121">

    <left>
        <VBox fx:id="vbLeftBar" maxHeight="Infinity" maxWidth="530" minWidth="100"
              spacing="5.0" >
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </BorderPane.margin>
        </VBox>
    </left>

    <center>
        <VBox fx:id="vbPDAInteraction" spacing="5.0" HBox.hgrow="ALWAYS">
            <HBox spacing="5" fx:id="hbCentre" VBox.vgrow="ALWAYS" maxHeight="Infinity">
                <VBox fx:id="vbDisplay" HBox.hgrow="ALWAYS" maxWidth="Infinity"></VBox>
            </HBox>
            <BorderPane.margin>
                <Insets bottom="5.0" right="5.0" top="5.0" />
            </BorderPane.margin>
        </VBox>
    </center>
</BorderPane>
</StackPane>