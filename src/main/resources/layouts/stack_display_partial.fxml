<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fx:id="spStack" fitToHeight="true" hbarPolicy="NEVER"
            maxHeight="Infinity" vbarPolicy="ALWAYS" xmlns="http://javafx.com/javafx/8.0.121">
    <AnchorPane maxHeight="Infinity" style="-fx-background-color: white">
        <HBox spacing="10" alignment="BOTTOM_CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <children>
        <VBox alignment="BOTTOM_CENTER">
            <children>
                <GridPane id="gpTopLocation" alignment="BOTTOM_RIGHT" maxWidth="50.0" vgap="-0.7">

                </GridPane>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="5.0" top="10.0"/>
            </padding>
        </VBox>
        <VBox alignment="BOTTOM_CENTER">
            <children>
                <GridPane id="gpStack" alignment="BOTTOM_RIGHT" maxWidth="50.0" vgap="-0.7">

                </GridPane>
            </children>
            <padding>
                <Insets bottom="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</HBox>
    </AnchorPane>
</ScrollPane>
